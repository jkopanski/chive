    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "tape": "^4.6.0",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.1",
