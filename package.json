{
  "name": "chive",
  "version": "0.0.1",
  "description": "Analysis setup for spice simulators",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jkopanski/chive.git"
  },
  "keywords": [
    "spice",
    "electronic",
    "simulation",
    "circuit"
  ],
  "authors": [
    "Jakub Kopanski <jkopanski@quivade.com> (https://github.com/jkopanski)"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jkopanski/chive/issues"
  },
  "scripts": {
    "bundle": "./node_modules/.bin/babel-node tools/run bundle",
    "lint": "eslint src tools",
    "start": "./node_modules/.bin/babel-node tools/run start",
    "test:init": "./node_modules/.bin/babel-node ./node_modules/.bin/tape  test/**/*.spec.js | ./node_modules/.bin/faucet",
    "test": "./node_modules/.bin/nyc npm test:init",
    "test:report": "./node_modules/.bin/nyc report",
    "test:watch": "npm test -- --watch"
  },
  "nyc": {
    "reporter": [
      "html",
      "lcov",
      "text"
    ],
    "extension": [
      ".jsx"
    ],
    "report-dir": "./test/coverage"
  },
  "dependencies": {
    "babel-polyfill": "6.9.0",
    "babel-runtime": "6.9.0",
    "humps": "1.1.0",
    "isomorphic-fetch": "2.2.1",
    "jsdom": "9.1.0",
    "material-ui": "0.15.2",
    "normalizr": "2.1.0",
    "query-string": "4.1.0",
    "ramda": "0.21.0",
    "ramda-fantasy": "^0.6.0",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-motion": "^0.4.2",
    "react-redux": "^4.4.0",
    "react-responsive-grid": "0.3.3",
    "react-router": "2.0.0",
    "react-swipeable-views": "^0.5.3",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.4.0",
    "redux-actions": "^0.9.1",
    "redux-logger": "^2.6.1",
    "redux-router": "^2.0.0-beta1",
    "redux-saga": "^0.10.0",
    "redux-thunk": "^2.0.1",
    "source-map-support": "0.4.2",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-middleware": "1.5.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.12.5",
    "enzyme": "^2.2.0",
    "eslint": "^3.1.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-loader": "*",
    "eslint-plugin-babel": "*",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "eslint_d": "^4.0.0",
    "expect": "^1.20.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "faucet": "0.0.1",
    "glob": "^7.0.3",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^8.0.2",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "nock": "^8.0.0",
    "nyc": "^7.0.0",
    "react-addons-test-utils": "^15.0.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.4",
    "redux-thunk": "^2.0.1",
