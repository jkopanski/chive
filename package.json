{
  "name": "chive",
  "version": "0.0.1",
  "description": "Analysis setup for spice simulators",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jkopanski/chive.git"
  },
  "keywords": [
    "spice",
    "electronic",
    "simulation",
    "circuit"
  ],
  "authors": [
    "Jakub Kopanski <jkopanski@quivade.com> (https://github.com/jkopanski)"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jkopanski/chive/issues"
  },
  "scripts": {
    "build": "babel-node tools/run build",
    "bundle": "babel-node tools/run bundle",
    "lint": "eslint src tools",
    "start": "babel-node tools/run start",
    "test:init": "babel-node ./node_modules/.bin/tape test/**/*.spec.js | faucet",
    "test": "nyc --reporter=text npm run test:init",
    "test:report": "nyc report",
    "test:watch": "npm test -- --watch",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "report-dir": "./test/coverage"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "babel-runtime": "6.23.0",
    "express": "4.14.1",
    "flow-static-land": "0.2.6",
    "fluture": "5.0.0",
    "humps": "2.0.0",
    "isomorphic-fetch": "2.2.1",
    "jsdom": "9.11.0",
    "material-ui": "0.17.0",
    "normalizr": "3.2.1",
    "query-string": "4.3.2",
    "ramda": "0.23.0",
    "ramda-fantasy": "0.7.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-motion": "0.4.7",
    "react-redux": "5.0.2",
    "react-responsive-grid": "0.3.4",
    "react-router": "2.8.1",
    "react-swipeable-views": "^0.10.8",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "3.6.0",
    "redux-actions": "1.2.1",
    "redux-logger": "2.8.1",
    "redux-router": "2.1.2",
    "redux-saga": "0.14.3",
    "source-map-support": "0.4.11"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.1.10",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "babel-template": "^6.23.0",
    "babel-types": "^6.23.0",
    "browser-sync": "^2.18.0",
    "chokidar": "^1.6.1",
    "coveralls": "^2.11.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.16.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-loader": "*",
    "eslint-plugin-babel": "*",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "eslint_d": "^4.0.0",
    "expect": "^1.20.0",
